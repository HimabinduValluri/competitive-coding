class Solution:
    def moreFrequent(self, arr, x, y):
        count_x=0
        count_y=0
        
        for n in arr:
            if n==x:
                count_x +=1
            elif n==y:
                count_y +=1
        
        if count_x>count_y:
            return x
        elif count_y>count_x:
            return y
        else:
            if x<y:
                return x
            else:
                return y
            
